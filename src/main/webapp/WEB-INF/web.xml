<web-app>
  <display-name>Archetype Created Web Application</display-name>  
	<servlet>
  	<servlet-name>addSubjectServlet</servlet-name>
  	<display-name>addSubjectServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlets.addSubjectServlet</servlet-class>
  </servlet>
	<servlet>
		<servlet-name>addTeacherServlet</servlet-name>
		<display-name>addTeacherServlet</display-name>
		<description></description>
		<servlet-class>com.servlets.addTeacherServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>deleteSubjectServlet</servlet-name>
		<display-name>deleteSubjectServlet</display-name>
		<description></description>
		<servlet-class>com.servlets.deleteSubjectServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>deleteTeacherServlet</servlet-name>
		<display-name>deleteTeacherServlet</display-name>
		<description></description>
		<servlet-class>com.servlets.deleteTeacherServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>manageClassServlet</servlet-name>
		<display-name>manageClassServlet</display-name>
		<description></description>
		<servlet-class>com.servlets.manageClassServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>addStudentServlet</servlet-name>
		<display-name>addStudentServlet</display-name>
		<description></description>
		<servlet-class>com.servlets.addStudentServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>deleteStudentServlet</servlet-name>
		<display-name>deleteStudentServlet</display-name>
		<description></description>
		<servlet-class>com.servlets.deleteStudentServlet</servlet-class>
	</servlet>
  <servlet-mapping>
  	<servlet-name>addSubjectServlet</servlet-name>
  	<url-pattern>/addSubjectServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>addTeacherServlet</servlet-name>
  	<url-pattern>/addTeacherServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>deleteSubjectServlet</servlet-name>
  	<url-pattern>/deleteSubjectServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>deleteTeacherServlet</servlet-name>
  	<url-pattern>/deleteTeacherServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>manageClassServlet</servlet-name>
  	<url-pattern>/manageClassServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>addStudentServlet</servlet-name>
  	<url-pattern>/addStudentServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>deleteStudentServlet</servlet-name>
  	<url-pattern>/deleteStudentServlet</url-pattern>
  </servlet-mapping>
